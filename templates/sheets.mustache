<div class="box informationbox well">
  <div class="box generalbox">
    <div class="form-group row">
      <div class="col-lg-12">
        <label for="inputSheetsFile">{{#str}}sheets_add_sheets, mod_amcquiz{{/str}}</label>
        <input id="inputSheetsFile" type="file" class="form-control" name="sheets" value="">
      </div>
    </div>
    <div class="form-group row">
      <div class="col-lg-12 text-lg-right">
        <button type="submit" class="btn btn-primary">
          <i class="fa fa-upload">&nbsp;{{#str}}upload, core{{/str}}</i>
        </button>
      </div>
    </div>
    <hr/>
    <div class="row">
      <div class="col-lg-12 text-lg-right">
        <button class="btn btn-danger btn-delete-sheets" type="button"><i class="fa fa-trash fa-fw"></i>{{#str}}sheets_delete_existing_sheets, mod_amcquiz{{/str}}</button>
      </div>
    </div>
  </div>
</div>
