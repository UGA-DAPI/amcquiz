<div class="box informationbox well">
  <div class="box generalbox">
    <div class="row">
      <div class="col-lg-12 text-lg-right">
        <form action="view.php?id={{cmid}}&current=sheets" method="post">
          <input type="hidden" name="action" value="delete-all-sheets">
          <button class="btn btn-danger btn-delete-sheets" type="submit">
            <i class="fa fa-trash fa-fw"></i>{{#str}}sheets_delete_existing_sheets, mod_amcquiz{{/str}}
          </button>
        </form>
      </div>
    </div>
    <hr/>
    <form action="view.php?id={{cmid}}&current=sheets" method="post" enctype="multipart/form-data">
      <input type="hidden" name="action" value="upload-sheets">
      <div class="form-group row">
        <div class="col-lg-12">
          <label for="inputSheetsFile">{{#str}}sheets_add_sheets, mod_amcquiz{{/str}}</label>
          <input id="inputSheetsFile" type="file" class="form-control" name="sheets">
        </div>
      </div>
      <div class="form-group row">
        <div class="col-lg-12 text-lg-right">
          <button type="submit" id="btn-file-upload" class="btn btn-primary">
            <i class="fa fa-upload fa-fw"></i>{{#str}}upload, core{{/str}}
          </button>
        </div>
      </div>
    </form>
    <hr/>
    <div class="row">
      <div class="col-lg-12 text-lg-right">
          <form action="view.php?id={{cmid}}&current=sheets" method="post">
              <input type="hidden" name="action" value="delete-unrecognized-sheets">
              <button type="submit" class="btn btn-danger">
                <i class="fa fa-trash fa-fw"></i>{{#str}}documents_delete_unrecognized_sheets, mod_amcquiz{{/str}}
              </button>
          </form>
      </div>
    </div>
    <hr/>
    <div class="row">
      <div class="col-lg-12">
        <h5>{{#str}}documents_unrecognized_sheets, mod_amcquiz{{/str}}</h5>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12 unrecognized-container">

      </div>
    </div>
  </div>
</div>
